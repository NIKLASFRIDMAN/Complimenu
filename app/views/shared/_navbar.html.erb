<div class="navbar-container" data-user-id= <%= @table.id %>>
<div class="navbar navbar-expand-sm navbar-light navbar-lewagon">
<% if !current_page?(root_path) %>
<a class="navbar-back" onclick="window.history.back()">
  <svg width="8" height="15" viewBox="0 0 8 15" fill="none" xmlns="http://www.w3.org/2000/svg">
  <path fill-rule="evenodd" clip-rule="evenodd" d="M7.12381 0L0.419048 7.04L0 7.5L0.419048 7.96L7.12381 15L8 14.08L1.73333 7.5L8 0.92L7.12381 0Z"/>
  </svg>
</a>
<%end%>
  <%= link_to table_path(@table), class: "navbar-brand" do %>
    <span class="logo">Complimenu</span>
  <% end %>
  <button  class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>


  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <% if user_signed_in? %>
        <% @users.each do |user| %>
          <li class="nav-item dropdown">
            <%= image_tag "https://kitt.lewagon.com/placeholder/users/ssaunier", class: "avatar dropdown-toggle", id: "navbarDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
          </li>
        <% end %>
      <% else %>
    <% end %>
    </ul>
  </div>
  </div>
</div>
